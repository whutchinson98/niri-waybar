[
  {
    "battery": {
      "bat": "BAT1",
      "format": "{capacity}% {icon}",
      "format-icons": [
        "",
        "",
        "",
        "",
        ""
      ],
      "interval": 60,
      "max-length": 25,
      "states": {
        "critical": 15,
        "warning": 30
      }
    },
    "clock": {
      "format": "{:%Y-%m-%d %H:%M}",
      "interval": 60,
      "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>"
    },
    "cpu": {
      "format": "{usage}% ",
      "tooltip": false
    },
    "custom/github": {
      "exec": "~/scripts/github-notifier.sh",
      "interval": 60,
      "return-type": "json"
    },
    "height": 24,
    "hyprland/workspaces": {
      "all-outputs": true,
      "disable-scroll": true,
      "format": "{name}: {icon}",
      "format-icons": {
        "1": "󰈹",
        "10": "",
        "2": "",
        "3": "",
        "4": "󰊯",
        "5": "󰎚",
        "7": "󰍡",
        "8": "",
        "9": "",
        "default": "",
        "focused": "",
        "urgent": ""
      }
    },
    "memory": {
      "format": "  {}%"
    },
    "modules-center": [],
    "modules-left": [
      "clock",
      "hyprland/workspaces",
      "cffi/hello-world"
    ],
    "modules-right": [
      "tray",
      "custom/github",
      "pulseaudio",
      "memory",
      "temperature#cpu",
      "battery"
    ],
    "cffi/hello-name": {
        "module_path": "target/debug/examples/libhello_world.so",
        "name": "Adam"
    },
    "cffi/hello-world": {
        "module_path": "target/debug/examples/libhello_world.so"
    },
    "pulseaudio": {
      "format": "{volume}% {icon} {format_source}",
      "format-bluetooth": "{volume}% {icon} {format_source}",
      "format-bluetooth-muted": " {icon} {format_source}",
      "format-icons": {
        "default": [
          "",
          "",
          ""
        ]
      },
      "format-muted": " {format_source}",
      "format-source": "{volume}% ",
      "format-source-muted": "",
      "on-click": "pavucontrol"
    },
    "spacing": 0,
    "temperature#cpu": {
      "critical-threshold": 70,
      "format": " {temperatureC}°C",
      "format-icons": [
        "",
        "",
        ""
      ],
      "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input"
    },
    "temperature#gpu": {
      "critical-threshold": 80,
      "format": "  {temperatureC}°C",
      "format-icons": [
        "",
        "",
        ""
      ],
      "hwmon-path": "/sys/class/hwmon/hwmon1/temp1_input"
    },
    "tray": {
      "icon-size": 15,
      "spacing": 8
    }
  }
]
